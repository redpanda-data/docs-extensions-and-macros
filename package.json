    "@octokit/rest": "^21.0.1",
    "algoliasearch": "^4.17.0",
    "chalk": "4.1.2",
    "gulp": "^4.0.2",
    "gulp-connect": "^5.7.0",
    "handlebars": "^4.7.8",
